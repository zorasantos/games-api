generator client {
  provider = "prisma-client-js"
   binaryTargets = ["native"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Games {
  id                String      @id @default(uuid())
  nome              String
  nomeCriador       String
  genero            String
  plataforma        String
  estudio           String
  dataLancamento    DateTime  @db.Date
  createdAt         DateTime  @default(now())
  updatedAt         DateTime  @updatedAt

  @@map("games")
}
